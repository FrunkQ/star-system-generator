    "prompts": {
        "systemPreamble": "You are an assistant expanding sci-fi star system details. Keep output concise, game-usable, and consistent with provided JSON and tags. Avoid changing facts. Use flavor only as garnish.",
        "fewShots": [
            { "input": { "entity": "Body", "tags": ["planet/super-earth"], "notes": "Rocky world with thin clouds." }, "output": "A high-gravity super-Earth with basaltic highlands and scattered rift valleys; settlements cluster in low-lying river basins sheltered from katabatic winds." }
        ],
        "perEntityPrompts": {
            "Body.description": "Describe the body's surface, hazards, and one adventure hook in 60-120 words, grounded in the JSON fields and tags."
        }
    },
    "viewPresets": {
        "defaultPlayerVisibility": {
            "discoveredBasics": true,
            "showTags": ["planet/habitable", "risk/*"],
            "hiddenFields": ["gmNotes", "areas", "resources"]
        },
        "overrides": [
            { "match": { "class": "artifact/*" }, "visibleFields": ["notes"], "hiddenFields": ["gmNotes"] }
        ]
    },
    "metrics": {
        "habitability": { "key": "habitability", "label": "Habitability", "min": 0, "max": 100, "default": 40 },
        "threat": { "key": "threat", "label": "Threat", "min": 0, "max": 100, "default": 20 },
        "tech": { "key": "tech", "label": "Tech Level", "min": 0, "max": 10, "default": 3 }
    },
    "classifier": {
        "maxClasses": 2,
        "minScore": 10,
        "rules": [
            { "when": { "any": [ { "gt": ["Teq_K", 1400] }, { "gt": ["tidalHeating", 10000] } ] }, "addClass": "planet/lava", "score": 100 },
            { "when": { "all": [ { "gt": ["hydrosphere.coverage", 0.8] }, { "between": ["Teq_K", 250, 370] } ] }, "addClass": "planet/ocean", "score": 100 },
            { "when": { "all": [ { "lt": ["Teq_K", 180] }, { "gt": ["hydrosphere.coverage", 0.1] }, { "not": { "gt": ["atm.pressure_bar", 2.0] } } ] }, "addClass": "planet/ice", "score": 100 },
            { "when": { "all": [ { "lt": ["hydrosphere.coverage", 0.1] }, { "between": ["Teq_K", 250, 400] } ] }, "addClass": "planet/desert", "score": 100 },
            { "when": { "all": [ { "lt": ["escapeVelocity_kms", 3.0] }, { "lt": ["hydrosphere.coverage", 0.01] } ] }, "addClass": "planet/barren", "score": 100 },
            { "when": { "all": [ { "gt": ["hydrosphere.coverage", 0.9] }, { "eq": ["atm.main", "H2"] } ] }, "addClass": "planet/hycean", "score": 110 },
            { "when": { "all": [ { "gt": ["mass_Me", 0.5] }, { "lt": ["mass_Me", 1.5] }, { "gt": ["hydrosphere.coverage", 0.5] }, { "eq": ["hydrosphere.composition", "water"] }, { "gt": ["atm.composition.O2", 0.1] } ] }, "addClass": "planet/earth-analogue", "score": 120 },
            { "when": { "all": [ { "gt": ["mass_Me", 1] }, { "eq": ["atm.main", "CO2"] } ] }, "addClass": "planet/carbon", "score": 105 },
            { "when": { "all": [ { "lt": ["mass_Me", 10] }, { "gt": ["stellarIrradiation", 1000] } ] }, "addClass": "planet/chthonian", "score": 110 },
            { "when": { "all": [ { "gt": ["mass_Me", 10] }, { "lt": ["Teq_K", 150] } ] }, "addClass": "planet/ammonia-clouds-gas-giant", "score": 80 },
            { "when": { "all": [ { "gt": ["mass_Me", 10] }, { "between": ["Teq_K", 150, 250] } ] }, "addClass": "planet/water-clouds-gas-giant", "score": 80 },
            { "when": { "all": [ { "gt": ["mass_Me", 10] }, { "between": ["Teq_K", 350, 800] } ] }, "addClass": "planet/cloudless-gas-giant", "score": 80 },
            { "when": { "all": [ { "gt": ["mass_Me", 10] }, { "gt": ["Teq_K", 1400] } ] }, "addClass": "planet/silicate-clouds-gas-giant", "score": 80 },
            { "when": { "all": [ { "lt": ["mass_Me", 10] }, { "gt": ["mass_Me", 1.5] }, { "lt": ["radius_Re", 1.2] } ] }, "addClass": "planet/iron", "score": 90 },
            { "when": { "all": [ { "gt": ["mass_Me", 95] }, { "any": [ { "between": ["a_AU", 0.0, 0.12] }, { "gt": ["Teq_K", 1000] } ] } ] }, "addClass": "planet/hot-jupiter", "score": 50 },
            { "when": { "all": [ { "gt": ["mass_Me", 95] }, { "between": ["Teq_K", 500, 1000] } ] }, "addClass": "planet/warm-jupiter", "score": 40 },
            { "when": { "all": [ { "gt": ["mass_Me", 95] }, { "lt": ["Teq_K", 250] } ] }, "addClass": "planet/cold-jupiter", "score": 30 },
            { "when": { "all": [ { "between": ["mass_Me", 2, 10] }, { "between": ["radius_Re", 1.25, 1.9] }, { "any": [ { "lt": ["atm.pressure_bar", 5] }, { "eq": ["atm.main", "CO2"] } ] } ] }, "addClass": "planet/super-earth", "score": 40 },
            { "when": { "all": [ { "between": ["mass_Me", 5, 20] }, { "between": ["radius_Re", 2.0, 4.0] }, { "any": [ { "gt": ["atm.pressure_bar", 5] }, { "any": [ { "eq": ["atm.main", "H2"] }, { "eq": ["atm.main", "He"] } ] } ] } ] }, "addClass": "planet/mini-neptune", "score": 40 },
            { "when": { "all": [ { "eq": ["tidallyLocked", 1] }, { "between": ["Teq_K", 200, 300] }, { "any": [ { "eq": ["stellarType", "M"] }, { "lt": ["a_AU", 0.2] } ] } ] }, "addClass": "planet/cold-eyeball", "score": 35 },
            { "when": { "all": [ { "eq": ["tidallyLocked", 1] }, { "gt": ["Teq_K", 300] } ] }, "addClass": "planet/hot-eyeball", "score": 35 },

            { "when": { "all": [ { "gt": ["mass_Me", 95] }, { "any": [ { "between": ["a_AU", 0.0, 0.12] }, { "gt": ["Teq_K", 1000] } ] } ] }, "addClass": "planet/hot-jupiter", "score": 50 },
            { "when": { "all": [ { "gt": ["mass_Me", 95] }, { "between": ["Teq_K", 500, 1000] } ] }, "addClass": "planet/warm-jupiter", "score": 40 },
            { "when": { "all": [ { "gt": ["mass_Me", 95] }, { "lt": ["Teq_K", 250] } ] }, "addClass": "planet/cold-jupiter", "score": 30 },

            { "when": { "between": ["radius_Re", 0.4, 1.6] }, "addClass": "planet/terrestrial", "score": 15 },
            { "when": { "all": [ { "gt": ["radius_Re", 3.5] }, { "gt": ["mass_Me", 10] } ] }, "addClass": "planet/gas-giant", "score": 15 },

            { "when": { "all": [ { "lt": ["Teq_K", 273] }, { "gt": ["tidalHeating", 50] }, { "gt": ["hydrosphere.coverage", 0.1] } ] }, "addClass": "planet/subsurface-ocean", "score": 90 },
            { "when": { "all": [ { "between": ["mass_Me", 10, 50] }, { "gt": ["Teq_K", 800] } ] }, "addClass": "planet/hot-neptune", "score": 60 },
            { "when": { "gt": ["mass_Me", 1000] }, "addClass": "planet/super-jupiter", "score": 60 },

            { "when": { "lt": ["age_Gyr", 0.1] }, "addClass": "planet/protoplanet", "score": 90 },
            { "when": { "lt": ["orbital_period_days", 1.0] }, "addClass": "planet/ultra-short-period", "score": 90 },



            { "when": { "all": [ { "lt": ["mass_Me", 0.1] }, { "gt": ["mass_Me", 0.01] } ] }, "addClass": "planet/dwarf-planet", "score": 50 },
            { "when": { "all": [ { "lt": ["density", 0.5] }, { "gt": ["mass_Me", 1] } ] }, "addClass": "planet/coreless", "score": 60 },
            { "when": { "gt": ["density", 1.5] }, "addClass": "planet/silicate", "score": 60 },

            { "when": { "lt": ["mass_Me", 0.5] }, "addClass": "planet/sub-earth", "score": 50 },
            { "when": { "all": [ { "gt": ["mass_Me", 10] }, { "lt": ["mass_Me", 50] } ] }, "addClass": "planet/mega-earth", "score": 50 },
            { "when": { "all": [ { "gt": ["mass_Me", 50] }, { "lt": ["mass_Me", 500] } ] }, "addClass": "planet/super-jupiter", "score": 50 },
            { "when": { "all": [ { "between": ["mass_Me", 10, 50] }, { "gt": ["Teq_K", 1000] } ] }, "addClass": "planet/hot-neptune", "score": 60 },
            { "when": { "all": [ { "gt": ["mass_Me", 95] }, { "gt": ["Teq_K", 1500] } ] }, "addClass": "planet/ultra-hot-jupiter", "score": 60 },
            { "when": { "all": [ { "between": ["mass_Me", 10, 50] }, { "gt": ["Teq_K", 1200] } ] }, "addClass": "planet/ultra-hot-neptune", "score": 60 },

            { "when": { "lt": ["rotation_period_hours", 2] }, "addClass": "planet/toroidal", "score": 90 },
            { "when": { "all": [ { "lt": ["rotation_period_hours", 6] }, { "gt": ["rotation_period_hours", 2] } ] }, "addClass": "planet/ellipsoid", "score": 80 },

            { "when": { "eq": ["has_ring_child", 1] }, "addClass": "planet/ringed", "score": 100 },
            { "when": { "all": [ { "eq": ["hydrosphere.composition", "methane"] }, { "gt": ["hydrosphere.coverage", 0.5] } ] }, "addClass": "planet/methane", "score": 90 },
            { "when": { "all": [ { "lt": ["escapeVelocity_kms", 1.0] }, { "gt": ["age_Gyr", 4] } ] }, "addClass": "planet/crater", "score": 70 },

            { "when": { "between": ["mass_Me", 4000, 8000] }, "addClass": "planet/brown-dwarf", "score": 120 },
            { "when": { "between": ["mass_Me", 1000, 4000] }, "addClass": "planet/sub-brown-dwarf", "score": 110 },
            { "when": { "all": [ { "between": ["mass_Me", 10, 50] }, { "lt": ["Teq_K", 200] } ] }, "addClass": "planet/ice-giant", "score": 80 },
            { "when": { "all": [ { "gt": ["radius_Re", 6] }, { "lt": ["mass_Me", 2] } ] }, "addClass": "planet/super-puff", "score": 90 },
            { "when": { "eq": ["parentId", null] }, "addClass": "planet/rogue", "score": 200 }
        ],
